<Window x:Class="LittleParser.Application.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LittleParser.Application"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
       <Menu>
            <MenuItem Header="_File">
                <MenuItem Header="_AddLogs" Click="DownloadFileForParsing"></MenuItem>
            </MenuItem>
       </Menu>
        <TextBlock x:Name="Status" Text="Download file"></TextBlock>
    </StackPanel>
</Window>
